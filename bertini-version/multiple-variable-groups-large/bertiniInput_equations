function f1,f2,f3,f4,f5,f6,f7,f8;
f1 = x_1*x_4 + x_2*x_5 + x_1*x_6;
f2 = x_3*x_4 + x_8*x_5 + x_7*x_6 + x_8 - x_1;
f3 = x_3*x_6 + x_2*x_3 + x_7*x_8;
f4 = x_4*x_5 - x_1;
f5 = x_2*x_3 + x_1*x_2 + x_7*x_8;
f6 = x_4*x_3 + x_7*x_2 + x_1*x_8 - x_2;
f7 = x_2*x_3 + x_7*x_2 + x_5*x_8 + x_8;
f8 = x_3*x_6*x_2 - 1;
